  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.18.1",
    "npm": "10.9.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "test:e2e": "cd e2e && npm test",
    "test:e2e:ui": "cd e2e && npm run test:ui",
    "test:e2e:headed": "cd e2e && npm run test:headed",
